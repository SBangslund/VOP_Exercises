<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="175.0" prefWidth="300.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1" fx:controller="vop_01b.FXMLDocumentController">
   <children>
      <HBox prefHeight="100.0" prefWidth="200.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <children>
            <VBox prefHeight="155.0" prefWidth="117.0" spacing="20.0">
               <children>
                  <RadioButton fx:id="radioEllipse" mnemonicParsing="false" onMouseClicked="#onClicked" text="Ellipse">
                     <padding>
                        <Insets right="45.0" />
                     </padding>
                  </RadioButton>
                  <RadioButton fx:id="radioRectangle" mnemonicParsing="false" onMouseClicked="#onClicked" prefHeight="17.0" prefWidth="136.0" text="Rectangle">
                     <padding>
                        <Insets right="50.0" />
                     </padding>
                  </RadioButton>
                  <RadioButton fx:id="radioCircle" mnemonicParsing="false" onMouseClicked="#onClicked" text="Circle" />
                  <RadioButton fx:id="radioSquare" mnemonicParsing="false" onMouseClicked="#onClicked" text="Square" />
               </children>
            </VBox>
            <VBox prefHeight="300.0" prefWidth="155.0" spacing="10.0">
               <children>
                  <HBox prefHeight="30.0" prefWidth="300.0">
                     <children>
                        <TextField fx:id="parameter01" prefWidth="40.0">
                           <HBox.margin>
                              <Insets right="20.0" />
                           </HBox.margin>
                        </TextField>
                        <TextField fx:id="parameter02" prefWidth="40.0" />
                     </children>
                  </HBox>
                  <Button mnemonicParsing="false" onAction="#handleGetInfo" text="Get info" />
                  <TextArea fx:id="resultBox" prefHeight="200.0" prefWidth="156.0" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
